// Slice , Splice

console.log("A", MyArray);

const myn1 = MyArray.slice(1,3) // Last range Include ni hoti only 1 and 2
console.log(myn1);

console.log("B", MyArray);  // slice original array ki value change ni krta.

const myn2 = MyArray.splice(1,3) // Last Range Include krta hai like 1,2,3. 

console.log("C", MyArray); // splice original array ki value change krdaita hai.
console.log(myn2);

